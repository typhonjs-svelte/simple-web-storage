{
  "name": "@typhonjs-svelte/simple-web-storage",
  "version": "0.0.0",
  "description": "Provides Svelte stores for persisting data to web storage (localStorage / sessionStorage).",
  "license": "MPL-2.0",
  "type": "module",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "sideEffects": false,
  "exports": {
    "./local": {
      "types": "./types/index.d.ts",
      "import": "./dist/local.js"
    },
    "./session": {
      "types": "./types/index.d.ts",
      "import": "./dist/session.js"
    },
    "./package.json": "./package.json"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.2",
    "@vitest/coverage-c8": "^0.32.2",
    "@vitest/ui": "^0.32.2",
    "rollup": "^3.25.3",
    "rollup-plugin-dts": "^5.3.0",
    "svelte": "^4.0.0",
    "tslib": "^2.6.0",
    "typescript": "^5.1.5",
    "vitest": "^0.32.2"
  },
  "peerDependencies": {
    "svelte": ">=4.x.x"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "/dist",
    "/types"
  ],
  "browserslist": [">5%", "not IE 11"],
  "scripts": {
    "build": "rollup -c",
    "eslint": "eslint .",
    "prepublishOnly": "npm run build && npm run test",
    "test": "vitest --coverage",
    "test-ui": "vitest --ui --coverage"
  }
}
